<!-- 主页 -->
<div class="product_wrap">
	{loop $product $cata}
	<div class="cat-floor__head">
		<div class="sub-categories">
			{if $cata['subclass']} {loop $cata['subclass'] $subc} <a
				href="{$subc['url']}">{$subc['name']}</a> {/loop} {/if} <a
				class="sub-categories_last" href="{$cata['url']}">全部</a>
		</div>
		<a href="{$cata['url']}" class="sub-name" id="{$cata['flag']}">{$cata['name']}</a>
	</div>
	<div class="product_item">
		<style type="text/css">
.deal-tile__detail .price {
	max-width: 101px;
	_width: 101px;
}

.deal-tile__title {
	padding: 10px 15px 0;
}

.deal-tile__detail {
	padding: 0 15px;
	height: 35px;
}
</style>
		{if $cata['product']} {loop $cata['product'] $item} {eval $icc++}
		<div class="t_area_out template2">
			<div class="t_area_in">
				<div class="deal-tile__cover">
					<div class="deal-tile__cover_img mt">{eval
						ui('iimager')->single($item['id'], $item['imgs'][0])}</div>
					<div class="deal-tile__cover_tag">
						{if $item['linkid']}<i><span title="多个套餐">多种套餐</span></i>{/if}
						{eval logic('product_tag')->html($item['id'])}
					</div>
				</div>
				<div class="deal-tile__title">
					<a href="?view={$item['id']}" target="_blank"
						title="{$item['name']}">{$item['name']}</a>
				</div>
				<div class="deal-tile__detail">
					<div class="price">&yen;{$item['nowprice']}</div>
					<div class="at_shuzi">
						<span>原价:</span><b class="prime_cost ">&yen;{$item['price']}</b>
					</div>
					<div class="deal_g">
						<a class="gotosee" href="?view={$item['id']}" target="_blank">去看看</a>
					</div>
				</div>
				<div class="deal-tile__extra">
					{if $item['presell']}
					<div class="yufu">
						<span>{TUANGOU_STR}价:</span><b>&yen;{$item['presell']['price_full']}</b>
					</div>
					{/if}
					<div id="tuanState" class="mb_0626">
						已售<b>{$item['sells_count']}</b>
					</div>
				</div>
			</div>
		</div>
		{/loop} {/if}
