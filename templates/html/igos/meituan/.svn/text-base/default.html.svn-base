	$(function() {

		var meishi_top = $("#meishi").offset().top - 400;
		var jiudian_top = $("#jiudian").offset().top - 400;
		var yule_top = $("#xiuxianyule").offset().top - 400;
		var travel_top = $("#lvyou").offset().top - 400;
		var life_top = $("#shenghuofuwu").offset().top - 400;
		var techan_top = $("#tutechan").offset().top - 400;

		var yinshi_top = $("#yinshiyuqi").offset().top - 400;
		var daijinquan_top = $("#daijinquan").offset().top - 400;
		var gongyipin_top = $("#gongyipin").offset().top - 400;
		var youxilei_top = $("#youxilei").offset().top - 400;
		//var chouijang_top = $("#choujiang").offset().top - 400;

		$(window).scroll(function() {
			var scroH = $(this).scrollTop();
			if (meishi_top > scroH) {
				setCss(0);
			}
			if (scroH >= meishi_top) {
				$(".hl_rightMenu").show();
			} else {
				$(".hl_rightMenu").hide();
			}
			//美食
			if (scroH >= meishi_top && scroH < jiudian_top) {
				setCss(1);
			}
			//酒店
			if (scroH >= jiudian_top && scroH < yule_top) {
				setCss(2);
			}
			//娱乐
			if (scroH >= yule_top && scroH < travel_top) {
				setCss(3);
			}
			//旅游
			if (scroH >= travel_top && scroH < life_top) {
				setCss(4);
			}
			//生活
			if (scroH >= life_top && scroH < techan_top) {
				setCss(5);
			}
			//特产
			if (scroH >= techan_top && scroH < yinshi_top) {
				setCss(6);
			}
			//银饰
			if (scroH >= yinshi_top && scroH < daijinquan_top) {
				setCss(7);
			}
			//代金券
			if (scroH >= daijinquan_top && scroH < gongyipin_top) {
				setCss(8);
			}
			//工艺品gongyipin
			if (scroH >= gongyipin_top && scroH < youxilei_top) {
				setCss(9);
			}
			//游戏
			if (scroH >= youxilei_top) {
				setCss(10);
			}
		});
	});

