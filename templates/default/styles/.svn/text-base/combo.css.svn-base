}
.site-body {
    margin: 0 auto;
    width: 980px;
}
.field-group {
    margin: 0 auto 8px;
    padding: 3px 10px 3px 110px;
    position: relative;
}
.field-group label {
    color: #333;
    font-size: 14px;
    left: 0;
    padding-top: 2px;
    position: absolute;
    text-align: right;
    width: 100px;
}
.field-group label.text {
    padding-top: 0;
}
.field-group label.normal {
    padding: 0;
    position: static;
    text-align: left;
    width: auto;
}
.field-group label .required {
    color: #c00;
}
.field-group .f-input {
    width: 280px;
}
