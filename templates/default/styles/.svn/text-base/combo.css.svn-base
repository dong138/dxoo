    font-family: "Microsoft YaHei","微软雅黑";
    font-size: 26px;
}
#content .mht-tips .more {
    font-size: 14px;
    line-height: 30px;
    margin-bottom: 20px;
}
.mine {
    position: relative;
    z-index: 1;
}
.mine .filter {
    border-bottom: 1px solid #dcdcdc;
    margin-bottom: 10px;
    padding: 5px 0 10px;
    z-index: 5;
}
.mine .filter li {
    float: left;
    font-size: 12px;
    margin: 0 5px 0 0;
}
.mine .filter a {
    display: block;
    padding: 1px 3px;
}
.mine .filter .current a {
    background: none repeat scroll 0 0 #DD66C2;
    border-radius: 3px;
    color: #fff;
    padding: 1px 7px;
}
.mine .spectip {
    color: #c30;
    float: right;
    margin-top: 14px;
}
.mine .bonus {
    position: relative;
}
#sidebar {
    float: right;
    width: 240px;
}
#sidebar h2 {
    font-size: 16px;
    padding-bottom: 9px;
}
#sidebar .sidebox {
    background-color: #eff8ff;
    border: 2px solid #89b4d6;
    margin-bottom: 10px;
    padding-bottom: 12px;
}
#sidebar .sidebox h3 {
    background: linear-gradient(to bottom, #fff, #eaecec) repeat scroll 0 0 rgba(0, 0, 0, 0);
    border-bottom: 1px solid #fff;
    box-shadow: 0 1px 0 #eaecec;
    font-size: 14px;
    height: 32px;
    line-height: 32px;
    padding: 0 12px;
}
#sidebar .sideinner {
    height: auto !important;
    min-height: 48px;
    padding: 12px 12px 6px;
}
#sidebar .mobile {
    height: 108px;
    padding: 0;
    position: relative;
}
#sidebar .mobile label a {
    display: block;
    height: 100%;
    outline: 0 none;
}
#sidebar .mobile a.tip {
    color: #666;
    display: block;
    font-size: 12px;
    left: 65px;
    outline: 0 none;
    position: absolute;
    top: 37px;
}
#sidebar .mobile a.tip:hover {
    text-decoration: none;
}
#sidebar .invite p {
    padding: 0 0 0 60px;
}
#sidebar .invite .click {
    font-size: 12px;
    padding: 16px 0 0;
    text-align: center;
}
#sidebar .service .sideinner {
    position: relative;
}
#sidebar .service p {
    color: #666;
    padding: 0 0 0 60px;
    position: absolute;
    top: 12px;
}
#sidebar .service strong {
    color: #86a649;
    display: block;
    font-size: 16px;
}
#sidebar .notice ul {
    margin-left: 16px;
}
#sidebar .notice li {
    list-style-position: outside;
    list-style-type: disc;
    padding: 3px 0;
}
#sidebar .banner {
    background: none repeat scroll 0 0 #e4f7ff;
    height: 125px !important;
    overflow: hidden;
    padding: 0;
}

#sidebar .consult .sideinner {
    background-position: 12px 12px;
    background-repeat: no-repeat;
}
#sidebar .consult p {
    color: #ccc;
    padding: 0 0 0 60px;
}
#sidebar .tips p, #sidebar .tips ul {
    color: #666;
    font-size: 12px;
    margin-bottom: 12px;
}
#sidebar .tips li {
    list-style: disc outside none;
    margin-left: 16px;
}
#sidebar .tips li strong {
    font-weight: 700;
}
#sidebar .credit {
    padding: 10px 10px 10px 15px;
}
#sidebar .cnnic {
    height: 60px;
    padding: 0;
}
#sidebar .commitment {
    height: 106px;
    padding: 0;
}
#sidebar .movie {
    height: 118px;
    padding: 0;
}
#sidebar .campus {
    height: 80px;
    padding: 0;
}
#sidebar .mt-app-download {
    border: 1px solid #e8e8e8;
    height: 300px;
    padding: 0;
}
#sidebar .banner-slider {
    height: 140px;
    overflow: hidden;
    padding: 0;
}
#sidebar .promotion {
    overflow: hidden;
    padding: 0;
}
#sidebar .alipayforevent, #sidebar .miaosha4 {
    height: 126px;
    overflow: hidden;
    padding: 0;
}
#sidebar .sbox {
    margin-bottom: 15px;
}
#sidebar .alipayforevent img, #sidebar .banner-slider img, #sidebar .campus img, #sidebar .cnnic img, #sidebar .commitment img, #sidebar .gift img, #sidebar .lottery img, #sidebar .miaosha4 img, #sidebar .mobile img, #sidebar .movie img, #sidebar .promotion img {
    vertical-align: bottom;
}
#sidebar .miaosha img {
    border: 1px solid #eae8e8;
    margin: 6px 0;
}
#sidebar .miaosha p {
    margin-bottom: 20px;
    position: relative;
}
#sidebar .miaosha strong {
    color: red;
}
#sidebar .side-box {
    border: 1px solid #e8e8e8;
    margin-bottom: 10px;
}
#sidebar .side-box--slider {
    position: relative;
}
#sidebar .side-box__title {
    background: none repeat scroll 0 0 #fff;
    padding: 9px 15px;
}
#sidebar .side-box img {
    vertical-align: top;
}
#sidebar .side-box--topic {
    background-color: #fff;
}
#sidebar .side-box--topic .topic-item-w {
}
#sidebar .side-box--topic .topic-item {
    background-repeat: no-repeat;
    display: block;
    height: 140px;
    overflow: hidden;
}
#sidebar .side-box--topic .topic-item:hover {
    text-decoration: none;
}
#sidebar .side-single {
    background: none repeat scroll 0 0 #fff;
    border: 1px solid #e8e8e8;
    margin-bottom: 10px;
    padding: 0 15px;
}
#sidebar .side-single h3 {
    margin-bottom: 12px;
}
#sidebar .side-single h3 .inner-link {
    color: #666;
    display: inline;
    font-size: 12px;
    margin-left: 10px;
    padding-right: 10px;
    position: relative;
}
#sidebar .side-single h3 .inner-link:hover {
    color: #DD66C2;
    text-decoration: underline;
}
#sidebar .side-single .inner-link:hover .arrow {
    background-position: -395px -8px;
}
#sidebar .side-single .inner-blk {
    border-bottom: 1px solid #f0f0f0;
    padding: 20px 0;
}
#sidebar .side-single .inner-blk img {
    vertical-align: top;
}
#sidebar .side-single .last {
    border-bottom: medium none;
}
#sidebar .side-single .lottery {
    color: #3e4545;
    padding-top: 0;
}
#sidebar .side-single .inner-side {
    padding: 0;
}
#sidebar .side-single .inner-img-blk {
    background-position: -15px 0;
    background-repeat: no-repeat;
    border-bottom: 1px solid #f0f0f0;
    color: #666;
    display: block;
    font-size: 12px;
    padding: 40px 0 16px 42px;
}
#sidebar .side-single .inner-img-blk:hover {
    color: #DD66C2;
    text-decoration: none;
}
#sidebar .side-single .inner-img-blk:hover .no-hover {
    color: #666;
}



