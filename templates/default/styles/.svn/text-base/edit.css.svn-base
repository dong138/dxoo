/*============================初始化==================================*/
body,h1,h2,h3,h4,h5,h6,hr,p,blockquote,dl,dt,dd,ul,ol,li,pre,form,fieldset,legend,button,input,textarea,th,td{margin:0;padding:0;word-wrap:break-word;}
body,html{ font-family:"微软雅黑"; color:#545454; font-size:12px;}
table{border-collapse:collapse;border-spacing:0;}
img{border:none;}
input.dialog{height:0; height:0; font-size:0; line-height:0; border:none;}
ul,ol,li{list-style:none;}
a { text-decoration:none; color:#2BB8AA;}
a:hover { color:#2BB8AA; text-decoration:underline;}

/**浮动样式**/
.left{float: left;}
.right{float: right;}
.clear{clear: both;}
.w{width: 100%; overflow: hidden; margin:auto;}

.edit_box {font-size: 14px;}
.edit_box h2{ height: 40px; line-height: 40px; border-bottom: 2px solid #ddd; font-size: 18px; color: #222; position: relative;}
.edit_box h2 span{ height: 40px; line-height: 40px; border-bottom: 2px solid #222222; position: absolute; left: 0px; bottom: -2px;}
.edit_info{width: 100%; height: 30px; line-height: 30px;padding-bottom: 10px;}
.edit_info label{color: red;}
.edit_title{width: 100%; height: 35px; line-height: 35px; background: #dedede; margin: 20px auto;}
.edit_title .edit_active{background: #FF8900 url(../img/arror.png) no-repeat right center; color: #FFFFFF; font-weight: bold;}
.edit_title li{float: left; width: 33.3333333%; text-align: center;}
.edit_box table{border: 1px solid #dedede; width: 100%;}
.edit_box table td{border-color: #dedede; border-left: none; border-right: none; padding-top: 20px; padding-bottom: 20px;}
.edit_box p{font-size: 12px; color:#999; }
.edit_box table h3{font-size: 14px;font-weight: 700; padding-bottom: 5px;}
.edit_box table a{display: block; width: 96px; height: 37px; text-align: center; line-height: 37px; background: url(../img/btn_bg_xtn.jpg) no-repeat; color: #FFFFFF; font-size: 14px; font-weight: bold;}
 table a:hover{text-decoration:underline;}
.edit_box .edit_ico1{background: url(../img/sp-retrieve-24.vb2c25215.png) no-repeat 0 -67px;width: 50px; height: 38px; margin-left: 100px; margin-right: 50px;}
.edit_box .edit_ico2{background: url(../img/sp-retrieve-24.vb2c25215.png) no-repeat 0 -17px;width: 50px; height: 38px; margin-left: 100px; margin-right: 50px;}
/* .edit_box span{display: block;} */
.edit_main{border: 1px solid #dedede; overflow: hidden;}
.edit_main table{width: 500px; margin: 20px auto; border: none;}
.edit_main table td{ padding: 10px;}
.pad_right{padding-right: 15px;}
#yanzhen,#tel{height: 24px; line-height: 24px; padding: 5px; border: 1px solid #aaa; width: 150px;}
.edit_staue{width: 290px; height: 50px; background: url(../img/icon-status.png) no-repeat 0 -105px; padding-left: 70px; margin: auto;}

.edit_staue3{width: 290px; height: 50px; background: url(../img/icon-status.png) no-repeat 0 -315px; padding-left: 70px; margin: auto;}



