.sendinfo_x h3{ height: 40px; line-height: 40px;}
.sendinfo_x .send_mail,.sendinfo_x span{float: left;}
 

.edit_box .changNum{  height: 37px;line-height: 37px; background: none !important; color: #2DB8AD; font-size: 12px; font-weight: bold; float: left; margin-left: 15px;}
 
 
 
.resend-message-tip {
    background: none repeat scroll 0 0 #fff6db;
    border: 1px solid #f5d8a7;
    box-shadow: 0 2px 1px 0 rgba(0, 0, 0, 0.15);
    color: #666;
    cursor: default;
    font-size: 12px;
    left: 750px;
    padding: 20px;
    position: absolute;
    top: 525px;
    width: 330px;
    z-index: 10;
	display:none;
}
.resend-message-tip .arrow--background, .verify .resend-message-tip .arrow--prospect {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    border-color: transparent transparent #fff6db;
    border-image: none;
    border-style: dashed dashed solid;
    border-width: 7px;
    font-size: 0;
    height: 0;
    left: 45px;
    line-height: 0;
    position: absolute;
    top: -14px;
    width: 0;
}
.resend-message-tip .arrow--background {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    border-color: transparent transparent #f5d8a7;
    border-image: none;
    border-style: dashed dashed solid;
    border-width: 8px;
    left: 5px;
    top: -16px;
}
.resend-message-tip .close {
    background: url("../img/sp-retrieve-24.vb2c25215.png") repeat scroll -18px 0 rgba(0, 0, 0, 0);
    cursor: pointer;
    height: 8px;
    position: absolute;
    right: 6px;
    top: 6px;
    width: 8px;
}
.resend-message-tip li {
    list-style: disc inside none;
}
.resend-message-tip .resend-error-tip {
    color: #f76120;
    margin: 10px 0 -5px 10px;
    position: relative;
}
.resend-message-tip .resend-message-btn {
    margin: 10px 0 10px 13px;
}

