 .edit_box .chang_num:hover{text-decoration: underline;}
 .sendinfo{ width: 900px; margin:20px auto; overflow: hidden; font-size: 16px;}
 
 .sendinfo_s{ background: #FFF6DB; width:860px; margin: auto; border: 1px solid #F5D8A7; padding: 20px 10px; margin-bottom: 20px; margin-top: 20px; line-height: 25px; text-align: center;}
 .sendinfo_s p{color: #666; font-size: 18px; padding-bottom: 10px;}
.sendinfo_s a{display: block; width: 96px; height: 37px; text-align: center; line-height: 37px; background: url(../img/btn_bg_xtn.jpg) no-repeat; color: #FFFFFF; font-size: 14px; font-weight: bold; margin: auto;}

.sendinfo_s a:hover{text-decoration: none; background: url(../img/btn_bg_hover.png) no-repeat;}
